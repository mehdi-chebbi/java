<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="900.0" style="-fx-background-color: #00ACFF;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PendingController">
    
 <children>
      <TableView fx:id="UserTableView" layoutX="13.0" layoutY="100.0" onMouseClicked="#clicked" prefHeight="338.0" prefWidth="875.0">
        <columns>
          <TableColumn fx:id="fncolumn" prefWidth="121.0" text="First name" />
          <TableColumn fx:id="lnamecolumn" prefWidth="102.0" text="Last name" />
            <TableColumn fx:id="concolumn" prefWidth="133.0" text="Created ON" />
            <TableColumn fx:id="lmodcolumn" prefWidth="130.0" text="Last modification" />
            <TableColumn fx:id="creabycolumn" prefWidth="120.0" text="Created By" />
            <TableColumn fx:id="modbycolumn" prefWidth="155.0" text="Modified By" />
            <TableColumn fx:id="rolecolumn" prefWidth="113.0" text="Role" />
        </columns>
      </TableView>
      
      <ImageView fx:id="logoiv" fitHeight="124.0" fitWidth="271.0" layoutX="115.0" layoutY="-6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo-topnet.png" />
         </image>
      </ImageView>
      <Label layoutX="411.0" layoutY="25.0" prefHeight="61.0" prefWidth="387.0" text="New Users Demands" textFill="WHITE">
         <font>
            <Font size="41.0" />
         </font>
      </Label>
      <Button fx:id="accept_button" layoutX="751.0" layoutY="485.0" mnemonicParsing="false" onAction="#Accept" prefHeight="43.0" prefWidth="112.0" stylesheets="@style.css" text="Accept" />
      <Button fx:id="delete_button" layoutX="616.0" layoutY="485.0" mnemonicParsing="false" onAction="#delete2" prefHeight="43.0" prefWidth="112.0" stylesheets="@style.css" text="Delete" />
      <ImageView fx:id="backk" fitHeight="70.0" fitWidth="88.0" layoutX="26.0" layoutY="16.0" onMouseClicked="#back" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8-back-arrow-100.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
